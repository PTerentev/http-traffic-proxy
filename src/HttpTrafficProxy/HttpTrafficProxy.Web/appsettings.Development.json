{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "HttpTrafficProxy": "Debug"
        }
    },
    "Application": {
        "UseAdvancedMode": true
    },
    "MessageBroker": {
        "DirectoryPath": "/data/broker-data",
        "RequestRetryCount": 5,
        "RequestRetryDelayMilliseconds": 25,
        "ConcurrentRequestCount": 128,
        "ResponseCacheCount": 128,
        "FileTimeToLiveMinutes": 5
    }
}
