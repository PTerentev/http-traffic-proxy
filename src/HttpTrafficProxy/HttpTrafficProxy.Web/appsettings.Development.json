{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Application": {
        "UseAdvancedMode": true
    },
    "MessageBroker": {
        "DirectoryPath": "\\Broker\\Files",
        "RequestRetryCount": 5,
        "RequestRetryDelayMilliseconds": 25,
        "ConcurrentRequestCount": 128,
        "ResponseCacheCount": 128,
        "FileTimeToLiveMinutes": 5
    }
}
